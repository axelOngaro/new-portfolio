ader {\n  display: flex;\n  position: fixed;\n  width: 100%;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 10%;\n  padding-top: 1rem;\n  font-size: 1.5rem;\n  z-index: 1;\n  opacity: 0;\n  transition: all 0.3s linear 0s;\n  -webkit-transition: all 0.5s linear 0s;\n  div {\n    cursor: pointer;\n  }\n}\n\n.dark-btn {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 10vh;\n}\n.moon {\n  fill: currentColor;\n}\n.sun {\n  fill: currentColor;\n}\nsvg {\n  height: 30%;\n}\n\n.hidden {\n  display: none;\n}\n","src/style/_home.scss":".home {\n  min-height: 100vh;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  font-size: 5rem;\n}\n\n.intro {\n  opacity: 0;\n  // visibility: hidden;\n}\n","src/style/_about.scss":".about {\n  min-height: 100vh;\n  font-size: 3rem;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  margin-bottom: 20%;\n}\n\n.about-text {\n  width: 50%;\n  padding: 6rem 0;\n}\n","src/style/_skills.scss":".skills {\n  min-height: 150vh;\n  background-color: darkblue;\n  color: white;\n  opacity: 0;\n}\n\n.horizontal-scroll {\n  min-height: 50vh;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  p {\n    font-size: 8rem;\n    padding: 0 10%;\n  }\n}\n\n.skills-title {\n  h2 {\n    font-size: 11rem;\n    font-weight: 400;\n  }\n  min-height: 30vh;\n  align-items: center;\n  margin-bottom: 20%;\n}\n\n.skills-title--1 {\n  position: relative;\n}\n.skills-title--2,\n.skills-title--3 {\n  position: absolute;\n  top: 0;\n  left: 0;\n  color: inherit;\n  opacity: 0;\n}\n","src/style/_work.scss":".work {\n  min-height: 100vh;\n  display: flex;\n  flex-direction: column;\n  color: inherit;\n  h2 {\n    font-size: 8rem;\n    font-weight: 400;\n  }\n}\n\n.work-list {\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  li {\n    padding: 10% 0;\n    font-size: 6rem;\n  }\n}\n\n.work-title {\n  min-height: 30vh;\n  align-items: center;\n  margin-top: 25vh;\n}\n\n.work-title--1 {\n  position: relative;\n}\n\n.work-title--2,\n.work-title--3 {\n  margin-top: 25vh;\n  position: absolute;\n  top: 0;\n  left: 0;\n  color: inherit;\n  opacity: 0;\n}\n","src/style/_contact.scss":".contact {\n  min-height: 100vh;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n}\n.email {\n  font-size: 5rem;\n  margin-bottom: 8rem;\n  line-height: 6rem;\n  cursor: pointer;\n  position: relative;\n}\n\n.email:hover .copy {\n  opacity: 1;\n}\n\n.copy {\n  font-size: 2rem;\n  width: 100%;\n  line-height: 6rem;\n  position: absolute;\n  margin-left: 10%;\n  opacity: 0;\n  transition: all ease 1s;\n}\n\n.contact-links {\n  font-size: 2rem;\n  transition: all 0.3s linear 0s;\n  -webkit-transition: all 0.5s linear 0s;\n  li {\n    padding: 1rem;\n  }\n}\n\n//media links\n.media-link {\n  position: relative;\n\n  &::after {\n    content: \"\";\n    position: absolute;\n    left: 0;\n    bottom: 0;\n    width: 100%;\n    height: 2px;\n    background-color: currentColor;\n\n    transform: scaleX(0);\n    transform-origin: right;\n    transition: transform 250ms ease-in;\n  }\n\n  &:hover::after {\n    transform: scaleX(1);\n    transform-origin: left;\n  }\n}\n\n.blured {\n  filter: blur(4px);\n}\n","src/style/_contactBanner.scss":".contact-banner {\n  min-height: 30vh;\n  margin-top: 10%;\n  color: inherit;\n  font-size: 15rem;\n  line-height: 80%;\n  width: 250%;\n  padding-left: 10%;\n}\n"},"lineCount":null}},"error":null,"hash":"7097016591db5e43ba2eff921d2886dc","cacheData":{"env":{}}}